{
  "expo": {
    "name": "Football coach",
    "slug": "football-coach",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/58c4d2ff-1987-46af-803d-c0231fc33c42.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/images/58c4d2ff-1987-46af-803d-c0231fc33c42.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF6347"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.erichsen.footballcoach",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/58c4d2ff-1987-46af-803d-c0231fc33c42.png",
        "backgroundColor": "#FF6347"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.FootballCoach",
      "permissions": [
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.SCHEDULE_EXACT_ALARM",
        "android.permission.POST_NOTIFICATIONS"
      ]
    },
    "web": {
      "favicon": "./assets/images/58c4d2ff-1987-46af-803d-c0231fc33c42.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/58c4d2ff-1987-46af-803d-c0231fc33c42.png",
          "color": "#FF6347",
          "defaultChannel": "task-reminders",
          "sounds": [],
          "mode": "production"
        }
      ]
    ],
    "owner": "nmh-holding",
    "scheme": "footballcoach",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "56add269-43c8-4368-9edc-3913dac2f57c"
      }
    }
  }
}
