{
  "name": "Natively",
  "version": "1.0.4",
  "main": "expo-router/entry",
  "scripts": {
    "dev": "EXPO_NO_TELEMETRY=1 expo start --clear --tunnel",
    "android": "EXPO_NO_TELEMETRY=1 expo start --android",
    "ios": "EXPO_NO_TELEMETRY=1 expo start --ios",
    "ios:metro": "expo start -c --dev-client",
    "web": "EXPO_NO_TELEMETRY=1 expo start --web",
    "qa": "npm run typecheck && npm run lint && npm test",
    "test": "jest",
    "e2e": "maestro test maestro-workspace-template --test-output-dir=artifacts",
    "e2e:ios:smoke": "bash scripts/maestro-smoke-mac.sh",
    "e2e:ios:activity-task-flow": "FLOW_PATH=e2e/flows/activity_task_flow_smoke.yaml REPEAT_COUNT=1 bash scripts/maestro-smoke-no-auth-mac.sh",
    "e2e:ios:auth-flow": "FLOW_PATH=e2e/flows/auth_flow_smoke.yaml REPEAT_COUNT=1 bash scripts/maestro-smoke-no-auth-mac.sh",
    "e2e:ios:paywall-flow": "FLOW_PATH=e2e/flows/paywall_gating_smoke.yaml REPEAT_COUNT=1 bash scripts/maestro-smoke-no-auth-mac.sh",
    "e2e:ios:library-add-to-tasks-flow": "FLOW_PATH=e2e/flows/library_add_to_tasks_smoke.yaml REPEAT_COUNT=1 bash scripts/maestro-smoke-no-auth-mac.sh",
    "e2e:ios:notifications-permission-flow": "FLOW_PATH=e2e/flows/notifications_permission_smoke.yaml REPEAT_COUNT=1 bash scripts/maestro-smoke-no-auth-mac.sh",
    "e2e:ios:role-based-ui-flow": "FLOW_PATH=e2e/flows/role_based_ui_smoke.yaml REPEAT_COUNT=1 bash scripts/maestro-smoke-no-auth-mac.sh",
    "e2e:ios:error-retry-flow": "FLOW_PATH=e2e/flows/error_retry_smoke.yaml REPEAT_COUNT=1 bash scripts/maestro-smoke-no-auth-mac.sh",
    "e2e:ios:fresh": "bash scripts/maestro-fresh-mac.sh",
    "e2e:ios:all": "FLOW_PATH=e2e/flows/_dev_client_bootstrap.yaml REPEAT_COUNT=1 bash scripts/maestro-smoke-no-auth-mac.sh && npm run e2e:ios:activity-task-flow && npm run e2e:ios:auth-flow && npm run e2e:ios:error-retry-flow && npm run e2e:ios:library-add-to-tasks-flow && npm run e2e:ios:notifications-permission-flow && npm run e2e:ios:paywall-flow && npm run e2e:ios:role-based-ui-flow",
    "e2e:android:activity-task-flow": "FLOW_PATH=e2e/flows/activity_task_flow_smoke.yaml REPEAT_COUNT=1 bash scripts/maestro-smoke-android.sh",
    "e2e:android:auth-flow": "FLOW_PATH=e2e/flows/auth_flow_smoke.yaml REPEAT_COUNT=1 bash scripts/maestro-smoke-android.sh",
    "e2e:android:paywall-flow": "FLOW_PATH=e2e/flows/paywall_gating_smoke.yaml REPEAT_COUNT=1 bash scripts/maestro-smoke-android.sh",
    "e2e:android:library-add-to-tasks-flow": "FLOW_PATH=e2e/flows/library_add_to_tasks_smoke.yaml REPEAT_COUNT=1 bash scripts/maestro-smoke-android.sh",
    "e2e:android:notifications-permission-flow": "FLOW_PATH=e2e/flows/notifications_permission_smoke.yaml REPEAT_COUNT=1 bash scripts/maestro-smoke-android.sh",
    "e2e:android:role-based-ui-flow": "FLOW_PATH=e2e/flows/role_based_ui_smoke.yaml REPEAT_COUNT=1 bash scripts/maestro-smoke-android.sh",
    "e2e:android:error-retry-flow": "FLOW_PATH=e2e/flows/error_retry_smoke.yaml REPEAT_COUNT=1 bash scripts/maestro-smoke-android.sh",
    "e2e:android:all": "bash scripts/maestro-smoke-android.sh",
    "build:web": "expo export -p web && npx workbox generateSW workbox-config.js",
    "build:android": "expo prebuild -p android",
    "lint": "eslint .",
    "focus:export-xlsx": "tsx scripts/focus/exportFocusXlsx.ts",
    "focus:import-xlsx": "tsx scripts/focus/importFocusXlsx.ts",
    "seed:holdtraening": "tsx scripts/holdtraening/importHoldtraening.ts",
    "seed:holdtraening:dry-run": "tsx scripts/holdtraening/importHoldtraening.ts --dry-run",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/runtime": "^7.26.9",
    "@bacons/apple-targets": "^3.0.2",
    "@expo/metro-runtime": "~6.1.1",
    "@expo/ngrok": "^4.1.3",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-navigation/drawer": "^7.1.1",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@supabase/supabase-js": "^2.84.0",
    "@types/difflib": "^0.2.7",
    "date-fns": "^4.1.0",
    "difflib": "^0.2.4",
    "eas": "^0.1.0",
    "expo": "~54.0.33",
    "expo-blur": "^15.0.6",
    "expo-constants": "~18.0.13",
    "expo-dev-client": "~6.0.20",
    "expo-font": "~14.0.11",
    "expo-glass-effect": "^0.1.1",
    "expo-haptics": "^15.0.6",
    "expo-image-picker": "^17.0.7",
    "expo-linear-gradient": "^15.0.6",
    "expo-linking": "^8.0.7",
    "expo-network": "^8.0.7",
    "expo-notifications": "~0.32.16",
    "expo-router": "~6.0.23",
    "expo-splash-screen": "^31.0.8",
    "expo-status-bar": "~3.0.7",
    "expo-symbols": "^1.0.6",
    "expo-system-ui": "^6.0.7",
    "expo-web-browser": "^15.0.6",
    "ical.js": "^2.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-css-interop": "^0.1.22",
    "react-native-edge-to-edge": "^1.7.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-iap": "^14.6.2",
    "react-native-maps": "1.20.1",
    "react-native-nitro-modules": "0.31.10",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.21.1",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "react-router-dom": "^7.1.3",
    "workbox-cli": "^7.4.0",
    "workbox-precaching": "^7.3.0",
    "workbox-webpack-plugin": "^7.3.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@testing-library/jest-native": "5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.12",
    "@typescript-eslint/eslint-plugin": "^8.12.1",
    "@typescript-eslint/parser": "^8.12.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "csv-parse": "^5.5.6",
    "eslint": "^9.39.2",
    "eslint-config-expo": "~10.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^15.14.0",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.17",
    "msw": "^1.3.5",
    "react-test-renderer": "19.1.0",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "webpack-cli": "^6.0.1"
  },
  "resolutions": {
    "@expo/prebuild-config": "latest"
  },
  "overrides": {
    "tmp": "^0.2.4"
  },
  "private": true
}
