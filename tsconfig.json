{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "jsx": "react-jsx",
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "workbox-config.js",
  ],
  "exclude": [
    "node_modules",
    "scripts/**",
    "supabase/functions/**",
    "head_profile.tsx",
    "**/*.dev.ts",
    "**/*.dev.tsx",
    "dist/**",
    "web-build/**",
    "_expo/**",
    "temp_qa_*/**",
    "**/*.rej",
    "profile.diff"
  ]
}
